 
package animal;


public class NewClass {
    interface Animal {
    void Sound();
}

class Cat implements Animal {
    public void Sound() {
        System.out.println("cat sound");
    }
}
public class Main {
    public static void main(String[] args) {
        animal.Animal a = new animal.Cat(); // instantiate a Cat object
        a.Sound(); // call the overridden method
    }
}
    
}





